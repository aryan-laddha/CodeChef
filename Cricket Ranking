#include <iostream>
using namespace std;

int main() {
    int t;
    int sum_a=0;
    int sum_b=0;

    std::cin >> t;
    while(t!=0){
        int a[3], b[3], c[3];
        for (int i = 0; i < 3; i++) {
          std::cin >> a[i];
          sum_a+=a[i];
        }
        for (int i = 0; i < 3; i++) {
          std::cin >> b[i];
          sum_b+=b[i];
        }

        
    if(sum_a>=sum_b)
	        {cout<<"A"<<endl;}

	    else{ cout<<"B"<<endl;}
    
    t--;

    sum_b=0;
    sum_a=0;
    }
	return 0;
}

