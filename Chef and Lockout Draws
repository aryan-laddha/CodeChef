 
#include <bits/stdc++.h>
using namespace std;

int main() {
	int T;
	cin>>T;
	while(T--)
	{
	    int arr[3];
	    for(int i =0 ; i <3 ; i++)
	    {
	        cin>>arr[i];
	    }
	    sort (arr,arr+3);
	    int sum = arr[0]+arr[1];
       if (sum==arr[2])
       {
        cout<<"YES"<<endl;
       }
       else{
         cout<<"NO"<<endl;
       }
      
	}
	return 0;
}
