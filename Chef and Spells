#include <iostream>
//#include<algorithm>
using namespace std;

int main() {
	int t;
	std::cin >> t;
	while(t!=0){
	    int x,y,z;
	    std::cin >> x>>y>>z;
	   // std::cout << std::max(a+b,b+c,c+a) << std::endl;
	      if(x+y>=x+z && x+y>=y+z)
	        cout<<x+y<<endl;
	    else if(y+z>=x+z && y+z>=x+y)
	        cout<<y+z<<endl;
	    else
	        cout<<x+z<<endl;
	    t--;
	}
	return 0;
}
