import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int T = sc.nextInt();
        int X, Y;

        while (T != 0) {
            X = sc.nextInt();
            Y = sc.nextInt();
            if (X > Y) {
                System.out.println(X - Y);
            } else {
                System.out.println(Y - X);
            }
            T--;
        }
    }
}
