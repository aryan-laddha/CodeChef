#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	int n,x,s=0;
	while(t!=0){
	    cin>>n>>x;
	    int a[n];
	    for (int i=1; i <= n; i++) {
	        cin>>a[i];
	        }
	   
	    for (int i =1 ; i <= n; i++) {
	        if(a[i]<x){
	        s=i;
	    }
	    }
	    cout << s << endl;
	    
	    t--;
	    n=0;
	    x=0;
	    s=0;
	}
	return 0;
}
