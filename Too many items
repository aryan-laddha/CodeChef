#include <iostream>
using namespace std;

int main() {
     int t;
     cin>>t;
     int n;
     while(t!=0){
  
         cin>>n;
         if(n%10==0){
             std::cout << n/10 << std::endl;
         }
         else{
             std::cout << (n/10)+1 << std::endl;
         }
         t--;
     }
	return 0;
}
